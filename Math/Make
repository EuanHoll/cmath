SRC = powlo powint sqrtlo sqrtint
SRCF = $(SRC:%=%.c)
OBJ = $(SRCF:%.c=%.o)
NAME = math.a

all: $(NAME)

$(NAME):
	gcc -Wall -Werror -Wextra -c $(SRCF) -g -I ./includes
	ar -rcsv $(NAME) $(OBJ)

clean:
	rm -f $(OBJ) ./includes/math.gch

mclean: $(NAME) clean

fclean: clean
	rm -f $(NAME)

re: fclean all
